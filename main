class Simulacion
  attr_accessor :nsvax, :cantidadmedicos, :tiempoProximaLlegada, :sumatoriaProximaSalida, :sumatoriaTiempoAtencion, :nsv, :nsa, :nsr, :nt, :tiempoProximaSalida
  def initialize(nsvMax, cantidadMedicos)
    @nsvax = nsvMax
    @cantidadmedicos = cantidadMedicos
    @tiempoProximaLlegada = 0
    @sumatoriaProximaSalida = 0
    @sumatoriaTiempoAtencion = 0
    @nsv = 0
    @nsa = 0
    @nsr = 0
    @nt = 0
    @tiempoProximaSalida = []
    for i in  0..cantidadmedicos-1
      @tiempoProximaSalida[i] = -1
    end
  end

  def printValues
    for i in 0..cantidadmedicos-1
      puts @tiempoProximaSalida[i]
    end
  end
end

simulacion = Simulacion.new(15,20)

simulacion.printValues()

